#include<iostream>
#include<vector>

using namespace std;

vector<int>rever(vector<int>v){
vector<int?newv;
  for(int i = 0; i<v.size()-1;i>=0;--1)
  {
  newv.push_back(v[i]);

  }
return newv;

}


int main()
{
  vector<int> v;
  v.push_back(1);
  v.push_back(2);
  v.push_back(4);
  v.push_back(9);
  v.push_back(16);
  
  vector<int> result = reverse(v);
  
  for(int i = 0; i<result.size(); ++i){
  
  cout<<result[i]<<",";
  }
  cout<<endl;
}

