3.3
#include<iostream>
#include<string>
#include<cmath>

using namespace std;

int main(){

cout<<"Enter three numbers: ";
double a, b, c;
cin<<a<<b<<c;

if(a<=c && a<=b){
cout<<a<<endl;
}

else if(b>=a &&b<=c)
{
cout<<b<<endl;
}
else{

cout<<c<<endl;
}

or most efficient way:
cout<<max(max(a,b),c)<<endl;

