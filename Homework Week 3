Problem 5:
int num; 
cout<<"numer:?";
cin>>num;

string s = "";

while(num!=0){
int quotient = num/2;
int remainder = num%2;

if(remainder ==0){s = "0"+s}
else{"1"  +s;}

num = quotient;


}

cout<<"The answer is"<<s<<endl;

