3.4
#include<iostream>
#include<string>

using namespace std;

int main(){

cout<<"Enter a letter grade: ";
string grade
cin>>grade;
if(grade == "B+")
cout<<"The numeric value is 3.3"<<endl;
if(grade == "B")
cout<<"The numeric value is 3.0"<<endl;
if(grade == B-)
cout<<"The numeric value is 2.7"<<endl;
